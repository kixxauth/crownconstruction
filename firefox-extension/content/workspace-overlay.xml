          </a>
        ]]></template>
        <fieldset id="jobs-siding" class="fform content-panel">
          <h4 class="fform section-header">Siding</h4>
        </fieldset>
        <template id="jobs_siding-template"><![CDATA[
          <h4 class="fform section-header">Siding</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Squares:</label>
                <input class="clean-text-input fform number-five" type="text"
                  name="<%= siding.squares.path %>" value="<%= siding.squares.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Type:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= siding.type.path %>" value="<%= siding.type.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Brand:</label>
                <input class="clean-text-input fform name" type="text"
                  name="<%= siding.brand.path %>" value="<%= siding.brand.value %>" />
              </p>
            </div>
            <div class="line">
              <p class="input-float"><label class="live-label">Style:</label>
                <input class="clean-text-input fform name" type="text"
                  name="<%= siding.style.path %>" value="<%= siding.style.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Color:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= siding.color.path %>" value="<%= siding.color.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Trim Color:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= siding.trim_color.path %>" value="<%= siding.trim_color.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="jobs-roofing" class="fform content-panel">
          <h4 class="fform section-header">Roofing</h4>
        </fieldset>
        <template id="jobs_roofing-template"><![CDATA[
          <h4 class="fform section-header">Roofing</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Squares:</label>
                <input class="clean-text-input fform number-five" type="text"
                  name="<%= roofing.squares.path %>" value="<%= roofing.squares.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Type:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= roofing.type.path %>" value="<%= roofing.type.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Brand:</label>
                <input class="clean-text-input fform name" type="text"
                  name="<%= roofing.brand.path %>" value="<%= roofing.brand.value %>" />
              </p>
            </div>
            <div class="line">
              <p class="input-float"><label class="live-label">Style:</label>
                <input class="clean-text-input fform name" type="text"
                  name="<%= roofing.style.path %>" value="<%= roofing.style.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Color:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= roofing.color.path %>" value="<%= roofing.color.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Tear Off:</label>
                <input class="clean-text-input fform" type="checkbox"
                  name="<%= roofing.tearoff.path %>" checked="<%= roofing.tearoff.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="jobs-permits" class="fform content-panel">
          <h4 class="fform section-header">Permits</h4>
        </fieldset>
        <template id="jobs_permits-template"><![CDATA[
          <h4 class="fform section-header">Permits</h4>
          <%
          var i = 0, item;
          for (; i < permits.length; i += 1) {
            item = permits[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Type:</label>
                <input class="clean-text-input fform label" type="text"
                  name="<%= item.type.path %>" value="<%= item.type.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Date Received:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= item.date_received.path %>" value="<%= item.date_received.value %>" />
              </p>
              <p class="input-float"><label class="live-label">ID Number:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= item.permit_num.path %>" value="<%= item.permit_num.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Phone Contact:</label>
                <input class="clean-text-input fform phone" type="text"
                  name="<%= item.phone.path %>" value="<%= item.phone.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="permits/permits" class="append fform button" title="Add a permit.">
            <span class="icon">+</span>
            Add a permit.
          </a>
        ]]></template>
      </div>
    </form><!-- job-view -->
  </div><!-- panel-jobs -->

  <!-- ### Personnel panel -->
  <div id="personnel" class="tab-panel">
    <!-- #### Personnel panel navigation -->
    <ul id="personnel-panel-navigation" class="panel-navigation">
      <li class="panel-navigation first personnel home">
        <a href="personnel/home" class="bbq panel-navigation">
          Personnel Panel
        </a>
      </li>
      <li class="panel-navigation personnel directory">
        <a href="personnel/directory" class="bbq panel-navigation">
          Directory
        </a>
      </li>
      <li class="panel-navigation personnel create">
        <a href="personnel/create" class="cmd panel-navigation">
          Create
        </a>
      </li>
      <li class="panel-navigation personnel view">
        <a href="personnel/view" class="bbq panel-navigation">
          View Employee
        </a>
      </li>
    </ul>
    <!-- #### Personnel home widget -->
    <div id="personnel-home" class="inner-tab-panel">
      <p>Personnel home.</p>
    </div>
    <!-- #### Personnel directory widget -->
    <div id="personnel-directory" class="personnel-panel inner-tab-panel">
      <ul id="personnel-group-list">
      </ul>
    </div>
    <!-- #### Personnel directory template -->
    <template id="personnel_directory-template"><![CDATA[
      <%
      var group_name, group, i, member;
      for (group_name in groups) {
        if (Object.prototype.hasOwnProperty.call(groups, group_name)) {
        group = groups[group_name]; %>
      <li class="personnel-group">
        <h4 class="personnel-group-name"><%= group_name %></h4>
        <ul class="personnel-group-members">
          <%
          for (i = 0; i < group.length; i += 1) {
            member = group[i]; %>
          <li class="employee">
          <a class="bbq employee-directory"
            href="personnel/view?key=<%= member.key %>">
            <%= member.name %>
          </a>
          </li>
          <%
          } %>
        </ul>
      </li>
      <%
        }
      } %>
    ]]></template>

    <!-- #### Personnel view widget -->
    <form id="personnel-view" class="fform personnel-panel inner-tab-panel">
      <div class="fform section">
        <fieldset id="personnel-name" class="fform content-panel">
          <h4 class="fform section-header">Name</h4>
        </fieldset>
        <template id="personnel_name-template"><![CDATA[
          <h4 class="fform section-header">Name</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">last:</label>
                <input class="clean-text-input fform lastname" type="text"
                  name="<%= name.last.path %>" value="<%= name.last.value %>" />
              </p>
              <p class="input-float"><label class="live-label">first:</label>
                <input class="clean-text-input fform firstname" type="text"
                  name="<%= name.first.path %>" value="<%= name.first.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="personnel-phones" class="fform content-panel">
          <h4 class="fform section-header">Phone Numbers</h4>
        </fieldset>
        <template id="personnel_phones-template"><![CDATA[
          <h4 class="fform section-header">Phone Numbers</h4>
          <%
          var i = 0, len = phones.length, phone;
          for (; i < len; i += 1) {
          phone = phones[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float">
                <label class="live-label">number:</label>
                <input class="clean-text-input fform phone" type="text"
                  name="<%= phone.phone.path %>" value="<%= phone.phone.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">label:</label>
                <input class="clean-text-input fform label" type="text"
                  name="<%= phone.label.path %>" value="<%= phone.label.value %>" />
              </p>
            </div>
          </div>
          <%
          } %>
          <a href="phones/phones" class="append fform button" title="Add another phone number.">
            <span class="icon">+</span>
            Add another phone number.
          </a>
        ]]></template>
      </div>
      <div class="fform section">
        <fieldset id="personnel-groups" class="fform content-panel">
          <h4 class="fform section-header">Groups</h4>
        </fieldset>
        <template id="personnel_groups-template"><![CDATA[
          <h4 class="fform section-header">Groups</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float">
                <label class="live-label">groups:</label>
                <input class="clean-text-input fform groups" type="text"
                  name="<%= groups.path %>" value="<%= groups.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="personnel-addresses" class="fform content-panel">
          <h4 class="fform section-header">Addresses</h4>
        </fieldset>
        <template id="personnel_addresses-template"><![CDATA[
          <h4 class="fform section-header">Addresses</h4>
          <%
          var i = 0, len = addresses.length, address;
          for (; i < len; i += 1) {
            address = addresses[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p>
                <label class="live-label">street:</label>
                <input class="clean-text-input fform street-address" type="text"
                  name="<%= address.street.path %>" value="<%= address.street.value %>" />
              </p>
            </div>
            <div class="line">
              <p class="input-float">
                <label class="live-label">city:</label>
                <input class="clean-text-input fform city" type="text"
                  name="<%= address.city.path %>" value="<%= address.city.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">state:</label>
                <input class="clean-text-input fform state-address" type="text"
                  name="<%= address.state.path %>" value="<%= address.state.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">zip:</label>
                <input class="clean-text-input fform zip" type="text"
                  name="<%= address.zip.path %>" value="<%= address.zip.value %>" />
              </p>
            </div>
          </div>
          <%
          } %>
          <a href="addresses/addresses" class="append fform button" title="Add another customer address.">
            <span class="icon">+</span>
            Add another address.
          </a>
        ]]></template>
      </div>
    </form><!-- personnel-view -->
  </div><!-- panel-personnel -->
</div><!-- tabset -->


<overlay>
<div id="workspace">
<!--
Application overlay HTML snippet.
=================================
-->

<!--
## Top menu bar
-->
<ul id="menu-bar">
</ul><!-- menu-bar -->

<!--
## Side bar
-->
<div id="sidebar">
  <img id="sidebar-logo" class="navigation" src="weblogo.png" width="80%" />
  <ul id="navigation">
    <li class="navigation section">
      <a href="panels/default" class="bbq navigation panels default"
        title="Open the home panel.">
        <span class="navigation twisty">+</span>
        Start
      </a>
    </li>
    <li class="navigation section">
      <h4 class="navigation">Search</h4>
    </li>
    <li class="navigation section">
      <a href="search/customers" class="bbq navigation search customers"
        title="Open the customer search panel to find customers.">
        <span class="navigation twisty">+</span>
        Customers
      </a>
    </li>
    <li class="navigation section">
      <a href="search/jobs" class="bbq navigation search jobs"
        title="Open the job search panel to find jobs.">
        <span class="navigation twisty">+</span>
        Jobs
      </a>
    </li>
    <li class="navigation section">
      <h4 class="navigation">Panels</h4>
    </li>
    <li class="navigation section">
      <a href="panels/customers" class="bbq navigation panels customers"
        title="Open the customers panel to find and create customers.">
        <span class="navigation twisty">+</span>
        Customers
      </a>
    </li>
    <li class="navigation section">
      <a href="panels/jobs" class="bbq navigation panels jobs"
        title="Open the jobs panel to find and create jobs.">
        <span class="navigation twisty">+</span>
        Jobs
      </a>
    </li>
    <li class="navigation section">
      <a href="panels/personnel" class="bbq navigation panels personnel"
        title="Open the personnel panel to find and create employees.">
        <span class="navigation twisty">+</span>
        Personnel
      </a>
    </li>
  </ul><!-- navigation -->
</div><!-- sidebar -->

<!--
## Main application tabset

  BBQ URL hashes for panel navigation should be in
  the form '#panels/'+ panel_name
-->

<div id="tabset">
  <!-- ### Default panel -->
  <div id="default" class="tab-panel">
    <!-- #### Default panel navigation -->
    <ul id="default-panel-navigation" class="panel-navigation">
      <li class="panel-navigation first default home">
        <a href="default/home" class="bbq panel-navigation">Start</a>
      </li>
      <li class="panel-navigation default controls">
        <a href="default/controls" class="bbq panel-navigation">Controls</a>
      </li>
    </ul>
    <!-- #### Default home widget -->
    <div id="default-home" class="inner-tab-panel">
      <p>Default home.</p>
    </div>
    <!-- #### Default controls widget -->
    <div id="default-controls" class="inner-tab-panel">
      <p>Control panel.</p>
    </div><!-- default-controls -->
  </div><!-- panel-default -->

  <!-- ### Customers panel -->
  <div id="customers" class="tab-panel">
    <!-- #### Customer panel navigation -->
    <ul id="customers-panel-navigation" class="panel-navigation">
      <li class="panel-navigation first customers home">
        <a href="customers/home" class="bbq panel-navigation">
          Customers Panel
        </a>
      </li>
      <li class="panel-navigation customers search">
        <a href="search/customers" class="bbq panel-navigation">
          Find
        </a>
      </li>
      <li class="panel-navigation customers create">
        <a href="customers/create" class="cmd panel-navigation">
          Create New
        </a>
      </li>
      <li class="panel-navigation customers view">
        <a href="customers/view" class="bbq panel-navigation">
          View Customer
        </a>
      </li>
    </ul>
    <!-- #### Customer home widget -->
    <div id="customers-home" class="inner-tab-panel">
      <p>Customers home.</p>
    </div>
    <!-- #### Customer view widget -->
    <form id="customers-view" class="fform inner-tab-panel">
      <div class="fform section">
        <fieldset id="customers-names" class="fform content-panel">
          <h4 class="fform section-header">Names</h4>
        </fieldset>
        <template id="customers_names-template"><![CDATA[
          <h4 class="fform section-header">Names</h4>
          <%
          var i = 0, len = names.length, name;
          for (; i < len; i += 1) {
          name = names[i];  %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">last:</label>
                <input class="clean-text-input fform lastname" type="text"
                  name="<%= name.last.path %>" value="<%= name.last.value %>" />
              </p>
              <p class="input-float"><label class="live-label">first:</label>
                <input class="clean-text-input fform firstname" type="text"
                  name="<%= name.first.path %>" value="<%= name.first.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="names/names" class="append fform button" title="Add another contact person.">
            <span class="icon">+</span>
            Add another contact person.
          </a>
        ]]></template>
        <fieldset id="customers-phones" class="fform content-panel">
          <h4 class="fform section-header">Phone Numbers</h4>
        </fieldset>
        <template id="customers_phones-template"><![CDATA[
          <h4 class="fform section-header">Phone Numbers</h4>
          <%
          var i = 0, len = phones.length, phone;
          for (; i < len; i += 1) {
          phone = phones[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float">
                <label class="live-label">number:</label>
                <input class="clean-text-input fform phone" type="text"
                  name="<%= phone.phone.path %>" value="<%= phone.phone.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">label:</label>
                <input class="clean-text-input fform label" type="text"
                  name="<%= phone.label.path %>" value="<%= phone.label.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="phones/phones" class="append fform button" title="Add another phone number.">
            <span class="icon">+</span>
            Add another phone number.
          </a>
        ]]></template>
      </div>
      <div class="fform section">
        <fieldset id="customers-addresses" class="fform content-panel">
          <h4 class="fform section-header">Addresses</h4>
        </fieldset>
        <template id="customers_addresses-template"><![CDATA[
          <h4 class="fform section-header">Addresses</h4>
          <%
          var i = 0, len = addresses.length, address;
          for (; i < len; i += 1) {
          address = addresses[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p>
                <label class="live-label">street:</label>
                <input class="clean-text-input fform street-address" type="text"
                  name="<%= address.street.path %>" value="<%= address.street.value %>" />
              </p>
            </div>
            <div class="line">
              <p class="input-float">
                <label class="live-label">city:</label>
                <input class="clean-text-input fform city" type="text"
                  name="<%= address.city.path %>" value="<%= address.city.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">state:</label>
                <input class="clean-text-input fform state-address" type="text"
                  name="<%= address.state.path %>" value="<%= address.state.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">zip:</label>
                <input class="clean-text-input fform zip" type="text"
                  name="<%= address.zip.path %>" value="<%= address.zip.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="addresses/addresses" class="append fform button" title="Add another customer address.">
            <span class="icon">+</span>
            Add another address.
          </a>
        ]]></template>
        <fieldset id="customers-emails" class="fform content-panel">
          <h4 class="fform section-header">Email Addresses</h4>
        </fieldset>
        <template id="customers_emails-template"><![CDATA[
          <h4 class="fform section-header">Email Addresses</h4>
          <%
          var i = 0, len = emails.length, email;
          for (; i < len; i += 1) {
          email = emails[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float">
                <label class="live-label">email:</label>
                <input class="clean-text-input fform email" type="text"
                  name="<%= email.email.path %>" value="<%= email.email.value %>" />
              </p>
              <p class="input-float">
                <label class="live-label">label:</label>
                <input class="clean-text-input fform label" type="text"
                  name="<%= email.label.path %>" value="<%= email.label.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="emails/emails" class="append fform button" title="Add another email address.">
            <span class="icon">+</span>
            Add another email address.
          </a>
        ]]></template>
      </div>
    </form><!-- customer-view -->
  </div><!-- panel-customers -->

  <!-- ### Jobs panel -->
  <div id="jobs" class="tab-panel">
    <!-- #### Jobs panel navigation -->
    <ul id="jobs-panel-navigation" class="panel-navigation">
      <li class="panel-navigation first jobs home">
        <a href="jobs/home" class="bbq panel-navigation">
          Jobs Panel
        </a>
      </li>
      <li class="panel-navigation jobs search">
        <a href="search/jobs" class="bbq panel-navigation">
          Find
        </a>
      </li>
      <li class="panel-navigation jobs create">
        <a href="search/customers?mode=newjob" class="cmd panel-navigation">
          Create New Job
        </a>
      </li>
      <li class="panel-navigation jobs view">
        <a href="jobs/view" class="bbq panel-navigation">
          View Job
        </a>
      </li>
    </ul>
    <!-- #### Jobs home widget -->
    <div id="jobs-home" class="inner-tab-panel">
      <p>Jobs home.</p>
    </div>
    <!-- #### Job view widget -->
    <form id="jobs-view" class="fform inner-tab-panel">
      <div class="fform section">
        <fieldset id="jobs-header" class="fform content-panel">
          <h4 class="fform section-header">Job Info</h4>
        </fieldset>
        <template id="jobs_header-template"><![CDATA[
          <h4 class="fform section-header">Job Info</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="fform input-float"><label class="live-label">Customer:</label>
                <span class="fform inactive subhead"><%= customer_name %></span>
              </p>
              <p class="input-float"><label class="live-label">Job ID:</label>
                <input class="clean-text-input fform tag" type="text"
                  name="<%= strname.path %>" value="<%= strname.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Sales Person:</label>
                <select class="clean-text-input fform" name="<%= sale_by.path %>">
                  <option selected="true" value="aKey">Place Holder</option>
                  <option value="secondKey">Second Holder</option>
                </select>
              </p>
              <p class="input-float"><label class="live-label">Production:</label>
                <select class="clean-text-input fform" name="<%= production_by.path %>">
                  <option selected="true" value="aKey">Place Holder</option>
                  <option value="secondKey">Second Holder</option>
                </select>
              </p>
            </div>
            <div class="line">
              <p class="input-float"><label class="live-label">description:</label>
                <input class="clean-text-input fform memo" type="text"
                  name="<%= description.path %>" value="<%= description.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="jobs-dates" class="fform content-panel">
          <h4 class="fform section-header">Dates</h4>
        </fieldset>
        <template id="jobs_dates-template"><![CDATA[
          <h4 class="fform section-header">Dates</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Contract Date:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= contractdate.path %>" value="<%= contractdate.value %>" />
              </p>
            </div>
            <div class="line">
              <h5 class="fform section-label">Start Date</h5>
              <p class="input-float"><label class="live-label">Estimated:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= est_startdate.path %>" value="<%= est_startdate.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Actual:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= startdate.path %>" value="<%= startdate.value %>" />
              </p>
              <h5 class="fform section-label">Complete Date</h5>
              <p class="input-float"><label class="live-label">Estimated:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= est_completedate.path %>" value="<%= est_completedate.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Actual:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= completedate.path %>" value="<%= completedate.value %>" />
              </p>
            </div>
            <div class="line">
              <h5 class="fform section-label">Handoff</h5>
              <p class="input-float"><label class="live-label">Scheduled:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= handoff.scheduled.path %>" value="<%= handoff.scheduled.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Completed:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= handoff.completed.path %>" value="<%= handoff.completed.value %>" />
              </p>
              <h5 class="fform section-label">Walkthrough</h5>
              <p class="input-float"><label class="live-label">Scheduled:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= walkthrough.scheduled.path %>" value="<%= walkthrough.scheduled.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Completed:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= walkthrough.completed.path %>" value="<%= walkthrough.completed.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
      </div>
      <div class="fform section">
        <fieldset id="jobs-estimate" class="fform content-panel">
          <h4 class="fform section-header">Estimate</h4>
        </fieldset>
        <template id="jobs_estimate-template"><![CDATA[
          <h4 class="fform section-header">Estimate</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Estimated By:</label>
                <select class="clean-text-input fform" name="<%= estimate_by.path %>">
                  <option selected="true" value="aKey">Place Holder</option>
                  <option value="secondKey">Second Holder</option>
                </select>
              </p>
              <p class="input-float"><label class="live-label">Estimate Date:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= estimate_date.path %>" value="<%= estimate_date.value %>" />
              </p>
            </div>
            <div class="line">
              <p class="input-float"><label class="live-label">Round Trip Miles:</label>
                <input class="clean-text-input fform number-three" type="text"
                  name="<%= roundtrip_miles.path %>" value="<%= roundtrip_miles.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Allotted Miles:</label>
                <input class="clean-text-input fform number-three" type="text"
                  name="<%= allotted_miles.path %>" value="<%= allotted_miles.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
        <fieldset id="jobs-profitandtaxes" class="fform content-panel">
          <h4 class="fform section-header">Finances</h4>
        </fieldset>
        <template id="jobs_profitandtaxes-template"><![CDATA[
          <h4 class="fform section-header">Finances</h4>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Estimated Profit:</label>
                <input class="clean-text-input fform number-five" type="text"
                  name="<%= estimated_profit.path %>" value="<%= estimated_profit.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Tax Labor:</label>
                <input class="clean-text-input fform number-five" type="text"
                  name="<%= taxlabor.path %>" value="<%= taxlabor.value %>" />
              </p>
            </div>
          </div>
        ]]></template>
      </div>
      <div class="fform section">
        <fieldset id="jobs-payments" class="fform content-panel">
          <h4 class="fform section-header">Payments</h4>
        </fieldset>
        <template id="jobs_payments-template"><![CDATA[
          <h4 class="fform section-header">Payments</h4>
          <%
          var i = 0, payment;
          for (; i < payments.length; i += 1) {
            payment = payments[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Due:</label>
                <input class="clean-text-input fform date" type="text"
                  name="<%= payment.due.path %>" value="<%= payment.due.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Memo:</label>
                <input class="clean-text-input fform memo" type="text"
                  name="<%= payment.memo.path %>" value="<%= payment.memo.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Amount:</label>
                <input class="clean-text-input fform money-6" type="text"
                  name="<%= payment.amount.path %>" value="<%= payment.amount.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="payments/payments" class="append fform button" title="Add a payment.">
            <span class="icon">+</span>
            Add a payment.
          </a>
          <%
          i = 0;
          for (; i < direct_pays.length; i += 1) {
            payment = direct_pays[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Direct Payment</label>
                <input class="clean-text-input fform memo" type="text"
                  name="<%= payment.path %>" value="<%= payment.value %>" />
              </p>
            </div>
          </div>
          <% } %>
          <a href="payments/direct_pays" class="append fform button" title="Add a direct payment.">
            <span class="icon">+</span>
            Add a direct payment.
          </a>
        ]]></template>
        <fieldset id="jobs-special_orders" class="fform content-panel">
          <h4 class="fform section-header">Special Orders</h4>
        </fieldset>
        <template id="jobs_special_orders-template"><![CDATA[
          <h4 class="fform section-header">Special Orders</h4>
          <%
          var i = 0, item;
          for (; i < special_orders.length; i += 1) {
            item = special_orders[i]; %>
          <div class="fform input-section">
            <div class="line">
              <p class="input-float"><label class="live-label">Description:</label>
                <input class="clean-text-input fform memo" type="text"
                  name="<%= item.description.path %>" value="<%= item.description.value %>" />
              </p>
              <p class="input-float"><label class="live-label">Vendor:</label>
                <input class="clean-text-input fform name" type="text"
                  name="<%= item.vendor.path %>" value="<%= item.vendor.value %>" />
